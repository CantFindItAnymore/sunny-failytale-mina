.header-container {
  height: 120rpx;
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: sticky;
  top: 0;
  z-index: 9;
}

.header-container>image {
  height: 60rpx;
  width: 60rpx;
}

.header-container>.header-input {
  height: 66rpx;
  width: 520rpx;
  border-radius: 50rpx;
  background: #f1f1f1;
  padding: 20rpx 26rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  font-size: 22rpx;
  color: #ccc;
}

.header-container>.header-input>image {
  height: 28rpx;
  width: 28rpx;
  margin-right: 26rpx;
}

.images-container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.logo-container {
  height: 102rpx;
  width: 100%;
  position: relative;
}

.logo-container>image:nth-child(1) {
  height: 100%;
  width: 100%;
}

.logo-container>image:nth-child(2) {
  height: 48rpx;
  width: 310rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

