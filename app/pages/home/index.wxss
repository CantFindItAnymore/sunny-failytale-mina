.calssify {
  /* height: 500rpx; */
  width: 100%;
  padding: 80rpx 50rpx 50rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.calssify>view {
  height: 180rpx;
  width: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
